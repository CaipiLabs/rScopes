{
  "name": "react-scopes",
  "version": "1.0.1",
  "description": "Intuitive, modulable & flexible state management system powered by reactive data components, multi-stores, scopes logic & flux architecture",
  "repository": "https://github.com/n8tz/react-scopes.git",
  "license": "MIT",
  "main": "dist/react-scopes.js",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "wpInherit": {
    "default": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "react-scopes",
        "externals": true,
        "production": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "staging": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "rootAlias": "react-scopes",
        "externals": true,
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "samples": {
      "rootFolder": "src",
      "basedOn": "ComponentSamples",
      "vars": {
        "rootAlias": "react-scopes",
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi && wpi :samples",
    "dev": "wpi-dev-server :samples --hot",
    "runBrowser": "start http://localhost:8080"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react-dom": "^16.3.2",
    "react": "^16.3.2"
  },
  "dependencies": {
    "rescope": "^4.4.5",
    "react-rescope": "^16.0.13",
    "rescope-spells": "^16.0.4"
  },
  "devDependencies": {
    "wpi-react-hmr-ssr": "^1.1.17",
    "webpack-inherit": "^1.6.7"
  }
}
